<title>Pokimon Data</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/stylesheets/pokimon.css">
    </head>
    <body>
        <!--[if lt IE 7]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="#">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
        <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Pokimon Name</th>
                <th scope="col">Details</th>
              </tr>
            </thead>
            <tbody>
                <% var i = 1 %> 
                <%pokimoves.pokemon.forEach(elementsdata=>{%>
                  <%console.log(elementsdata)%>
                <tr>
                <th scope="row"><%=i++%></th>
                <td><%=elementsdata.pokemon.name%></td>
                <td>
                        <% var linkurl= elementsdata.pokemon.url%>
                        <% var res= linkurl.split("/")%>
                        <% var mainResult= res.slice(6,7)%>
                        <form action="/data/<%=mainResult%>" method="post">
                            <input type="submit" value="Details"class="btn btn-primary">
                        </form>
                </td>
              </tr>
              <%})%>
            </tbody>
          </table>



















        
        <script src="" async defer></script>
    </body>
</html>